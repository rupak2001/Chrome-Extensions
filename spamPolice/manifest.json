{
  "manifest_version": 3,
  "name": "Spam Police",
  "description": "Base Level Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "body.html",
    "default_icon": "spam.png"
  },
  "permissions": ["tabs", "activeTab"],
  "background": {
    "service_worker": "service_worker.js"
  }
  
}
