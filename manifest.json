{
  "manifest_version": 3,
  "name": "Link Chimney",
  "description": "Base Level Extension",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "hello.html",
    "default_icon": "extractor.png"
  },
  "permissions": ["tabs", "activeTab"]
}
